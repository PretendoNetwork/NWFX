{
	"compilerOptions": {
		"strict": true,
		"sourceMap": true,
		"resolveJsonModule": true,
		"module": "commonjs",
		"esModuleInterop": true,
		"moduleResolution": "node",
		"baseUrl": "src",
		"outDir": "dist",
		"allowJs": true,
		"downlevelIteration": true,
		"lib": ["dom", "DOM.Iterable", "es2022"],
		"noEmitOnError": true,
		"paths": {
			"@/*": ["./*"]
		}
	},
	"include": ["src"]
}